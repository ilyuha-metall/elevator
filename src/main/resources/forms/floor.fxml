<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.shape.Rectangle?>

<GridPane fx:id="floorPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
          prefHeight="64.0" prefWidth="120.0"
          xmlns:fx="http://javafx.com/fxml/1" fx:controller="test.elevator.gui.FxFloorController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="2.0" minHeight="2.0" prefHeight="2.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="60.0" minHeight="60.0" prefHeight="60.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="2.0" minHeight="2.0" prefHeight="2.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <StackPane fx:id="cabinPlace" prefHeight="150.0" prefWidth="200.0" GridPane.rowIndex="1"/>
        <Rectangle arcWidth="5.0" height="2.0" stroke="BLACK" strokeType="INSIDE" width="120.0"
                   GridPane.columnSpan="2"/>
        <Rectangle arcWidth="5.0" height="2.0" stroke="BLACK" strokeType="INSIDE" width="120.0" GridPane.columnSpan="2"
                   GridPane.rowIndex="2"/>
        <Rectangle arcWidth="5.0" fill="#444c54" height="60.0" opacity="0.26" stroke="BLACK" strokeType="OUTSIDE"
                   width="60.0" GridPane.rowIndex="1"/>
    </children>
</GridPane>
